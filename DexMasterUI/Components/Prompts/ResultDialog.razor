<MudDialog>
    <DialogContent>
        <MudStack>
            <MudText>@Prompt.PromptResult</MudText>
        </MudStack>
    </DialogContent>
    <DialogActions>
        <MudButton Color="Color.Primary" OnClick="Submit">Ok</MudButton>
    </DialogActions>
</MudDialog>

@code {
    [CascadingParameter]
    public MudDialogInstance MudDialog { get; set; }

    [Parameter]
    public Prompt Prompt { get; set; }

    private void Submit()
    {
        MudDialog.Close(DialogResult.Ok(true));
    }
}
